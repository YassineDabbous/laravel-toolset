{
    "version": "1.0.0",
    "name": "yassinedabbous/laravel-toolset",
    "description": "helpers to build a robust app",
    "type": "library",
    "authors": [
        {
            "name": "yassine-dabbous",
            "email": "yassine.dabbous@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Yaseen\\Toolset\\": "src/",
            "Yaseen\\Toolset\\Database\\Factories\\": "database/factories",
            "Yaseen\\Toolset\\Database\\Seeders\\": "database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yaseen\\Toolset\\ToolsetServiceProvider"
            ]
        }
    }
}